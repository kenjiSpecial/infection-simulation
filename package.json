{
	"name": "ts-parcel-as-lib-builder",
	"version": "0.0.0",
	"description": "Example project: Parcel as js Library builder",
	"main": "src/index.ts",
	"author": "satotake",
	"license": "MIT",
	"scripts": {
		"build": "rm -rf dist/*.* && parcel build dev/index.html --no-source-maps --public-url ./",
		"doc": "typedoc --out docs --theme minimal --mode file src",
		"start": "rm -rf dist/*.* && parcel serve dev/index.html --no-hmr",
		"lint": "tslint -t stylish --project \"tsconfig.json\"",
		"fix": "tslint --fix  src/**/*.ts{,x}"
	},
	"dependencies": {
		"@types/dat.gui": "^0.7.5",
		"@types/navigo": "^7.0.1",
		"bowser": "^2.9.0",
		"dat.gui": "^0.7.7",
		"gsap": "^3.2.6",
		"navigo": "^7.1.2",
		"three": "^0.115.0"
	},
	"devDependencies": {
		"glslify-bundle": "^5.1.1",
		"glslify-deps": "^1.3.1",
		"parcel-bundler": "^1.12.3",
		"prettier": "^1.18.2",
		"sass": "^1.26.5",
		"tslint": "^5.20.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-microsoft-contrib": "^6.2.0",
		"tsutils": "^3.17.1",
		"typedoc": "^0.15.0",
		"typescript": "^3.6.3"
	}
}
